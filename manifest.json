{
	"manifest_version": 2,

	"name": "Facebook Status Filter",

	"description": "Filter Facebook status based on trained data by YOU!",

	"version": "1.0",

	"content_scripts": [
		{
			"matches": ["http://facebook.com/*", "http://www.facebook.com/*", "https://facebook.com/*", "https://www.facebook.com/*"],

			"js": ["facebookStatusFilter.js"]
		}

	]


}